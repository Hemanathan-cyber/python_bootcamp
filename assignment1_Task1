a=[3,5,[3,56],3,[3,7,'zen',4.6],'guvi',5,2,'pit']
print(len(a))
no_of_sublist=0
maxListIndex=0
for i in a:
    if isinstance(i,list):
        no_of_sublist+=1
        if maxListIndex < len(i):
            maxListIndex=len(i)
            index=a.index(i)
print("number of sublist in lis:",no_of_sublist)
print("sub list with max length:",a[index])
print("index of sub list with max length:",index)
print("first element of the sub list with maxlength:",a[index][0])
